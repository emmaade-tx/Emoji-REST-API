{
    "name": "demola/emoji",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework","rest","router", "psr7"],
    "homepage": "https://github.com/andela-araimi/Emoji-REST-API.git",
    "license": "MIT",
    "authors": [
        {
             "name": "Raimi Ademola",
            "email": "ademolaraimi.nig@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "^5.2",
        "firebase/php-jwt": "^3.0",
        "vlucas/phpdotenv": "^2.2",
        "satooshi/php-coveralls": "^1.0",
        "mikey179/vfsStream": "^1.6"
    },
     "require-dev" : {
         "phpunit/phpunit": "^4.8.22",
         "mockery/mockery": "0.9.*"
        },
   "autoload": {
        "psr-4": {
            "Demo\\": "src"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Demo\\": "tests"
        },
        "classmap": ["tests/"]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
